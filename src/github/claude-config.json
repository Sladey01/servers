{
   "mcpServers": {
     "brave-search": {
       "command": "npx",
       "args": [
         "-y",
         "@modelcontextprotocol/server-brave-search"
       ],
       "env": {
         "BRAVE_API_KEY": "apitoken"
       }
     },
     "github": {
       "command": "npx",
       "args": [
         "-y",
         "@modelcontextprotocol/server-github"
       ],
       "env": {
         "GITHUB_PERSONAL_ACCESS_TOKEN": "apitoken"
       }
     },
     "snyk": {
       "command": "npx",
       "args": [
         "-y",
         "@modelcontextprotocol/server-snyk"
       ],
       "env": {
         "SNYK_API_KEY": "apitoken",
         "GITHUB_REPOSITORY": "Sladey01/servers"
       }
     }
   }
}